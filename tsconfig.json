{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "dist",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "removeComments": false,
    "noImplicitAny": false,
    "lib": [
      "es2017",
      "dom"
    ],
    "preserveConstEnums": true,
    "suppressImplicitAnyIndexErrors": true,
    "types": [
      "node",
      "dojo",
      "arcgis-js-api"
    ]
  },
	"include": [
    "app/**/*"
  ],
	"files": [
    "app/options.ts"
  ]
}
